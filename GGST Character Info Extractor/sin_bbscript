  endSubroutine: 

beginSubroutine: s32'RTL_Move_Accel'
  clearRegisteredUponCode: (FRAME_STEP)
  upon: (FRAME_STEP)
    ifOperation: (IS_EQUAL), Mem(105), Val(3)
      velocityXPercentEachFrame: 85
      velocityYPercentEachFrame: 85
      endIf: 
    ifOperation: (IS_EQUAL), Mem(105), Val(12)
      velocityXPercentEachFrame: 80
      velocityYPercentEachFrame: 80
      endIf: 
    ifOperation: (IS_EQUAL), Mem(105), Val(18)
      velocityXPercentEachFrame: 100
      velocityYPercentEachFrame: 100
      endIf: 
    if: Mem(50)
      ifOperation: (IS_LESSER_OR_EQUAL), Mem(18), Val(300000)
        ifOperation: (IS_EQUAL), Mem(105), Val(0)
          velocityXPercent: 50
          velocityYPercent: 50
          endIf: 
        endIf: 
      ifNot: Mem(30)
        velocityXPercentEachFrame: 90
        velocityYPercentEachFrame: 90
        endIf: 
      endIf: 
    endUpon: 
  endSubroutine: 

