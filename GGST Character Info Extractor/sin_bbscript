beginSubroutine: s32'RTL_Rush_Start'
  enableGatlings: 0
  upon: 36
    if: Mem(45)
      if: Mem(47)
        clearRegisteredUponCode: 36
        ifOperation: (IS_EQUAL), Mem(47), Val(5)
          enableGatlings: 1
          endIf: 
        ifOperation: (IS_LESSER), Mem(46), Val(3)
          ifOperation: (IS_EQUAL), Mem(48), Mem(32)
            ifOperation: (IS_EQUAL), Mem(47), Val(1)
              if: Mem(30)
                gotoLabelRequests: s32'1'
                endIf: 
              else: 
                gotoLabelRequests: s32'4'
                endElse: 
              endIf: 
            ifOperation: (IS_EQUAL), Mem(47), Val(2)
              if: Mem(30)
                gotoLabelRequests: s32'2'
                endIf: 
              endIf: 
            ifOperation: (IS_EQUAL), Mem(47), Val(3)
              if: Mem(30)
                gotoLabelRequests: s32'3'
                endIf: 
              else: 
                gotoLabelRequests: s32'6'
                endElse: 
              endIf: 
            ifOperation: (IS_EQUAL), Mem(47), Val(4)
              gotoLabelRequests: s32'4'
              endIf: 
            ifOperation: (IS_EQUAL), Mem(47), Val(6)
              gotoLabelRequests: s32'6'
              endIf: 
            ifOperation: (IS_EQUAL), Mem(47), Val(7)
              gotoLabelRequests: s32'7'
              endIf: 
            ifOperation: (IS_EQUAL), Mem(47), Val(8)
              gotoLabelRequests: s32'8'
              endIf: 
            ifOperation: (IS_EQUAL), Mem(47), Val(9)
              gotoLabelRequests: s32'9'
              endIf: 
            endIf: 
          else: 
            ifOperation: (IS_EQUAL), Mem(47), Val(1)
              gotoLabelRequests: s32'3'
              endIf: 
            ifOperation: (IS_EQUAL), Mem(47), Val(2)
              gotoLabelRequests: s32'2'
              endIf: 
            ifOperation: (IS_EQUAL), Mem(47), Val(3)
              gotoLabelRequests: s32'1'
              endIf: 
            ifOperation: (IS_EQUAL), Mem(47), Val(4)
              gotoLabelRequests: s32'6'
              endIf: 
            ifOperation: (IS_EQUAL), Mem(47), Val(6)
              gotoLabelRequests: s32'4'
              endIf: 
            ifOperation: (IS_EQUAL), Mem(47), Val(7)
              gotoLabelRequests: s32'9'
              endIf: 
            ifOperation: (IS_EQUAL), Mem(47), Val(8)
              gotoLabelRequests: s32'8'
              endIf: 
            ifOperation: (IS_EQUAL), Mem(47), Val(9)
              gotoLabelRequests: s32'7'
              endIf: 
            endElse: 
          storeValue: Mem(45), Val(0)
          storeValue: Mem(47), Val(0)
          endIf: 
        endIf: 
      endIf: 
    endUpon: 
  endSubroutine: 

beginSubroutine: s32'RTL_Rush_End'
  recoveryState: 
  if: Mem(30)
    upon: (TOUCH_GROUND)
      clearRegisteredUponCode: (TOUCH_GROUND)
      gotoLabelRequests: s32'end_land'
      endUpon: 
    resetGravity: 
    setGravity: 3000
    ifNot: Mem(50)
      gotoLabelRequests: s32'end_air'
      endIf: 
    else: 
      gotoLabelRequests: s32'end_air123'
      endElse: 
    endIf: 
  else: 
