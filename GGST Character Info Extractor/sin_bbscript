beginSubroutine: s32'RTL_Rush_Input'
  if: Mem(372)
    storeValue: Mem(373), Val(0)
    endIf: 
  upon: 12
    checkInput: 29
    if: Mem(0)
      ifOperation: (IS_LESSER), Mem(46), Val(3)
        clearRegisteredUponCode: 12
        endIf: 
      storeValue: Mem(48), Mem(32)
      checkInput: (INPUT_1)
      if: Mem(0)
        storeValue: Mem(47), Val(1)
        endIf: 
      checkInput: (INPUT_2)
      if: Mem(0)
        storeValue: Mem(47), Val(2)
        endIf: 
      checkInput: (INPUT_3)
      if: Mem(0)
        storeValue: Mem(47), Val(3)
        endIf: 
      checkInput: (INPUT_4)
      if: Mem(0)
        storeValue: Mem(47), Val(4)
        endIf: 
      checkInput: (INPUT_5)
      if: Mem(0)
        storeValue: Mem(47), Val(5)
        clearRegisteredUponCode: 12
        endIf: 
      checkInput: (INPUT_6)
      if: Mem(0)
        storeValue: Mem(47), Val(6)
        endIf: 
      checkInput: (INPUT_7)
      if: Mem(0)
        storeValue: Mem(47), Val(7)
        endIf: 
      checkInput: (INPUT_8)
      if: Mem(0)
        storeValue: Mem(47), Val(8)
        endIf: 
      checkInput: (INPUT_9)
      if: Mem(0)
        storeValue: Mem(47), Val(9)
        endIf: 
      endIf: 
    if: Mem(372)
      ifNot: Mem(373)
        setRandom: 2, 373, 0, 1, 0, 12
        ifOperation: (IS_EQUAL), Mem(373), Val(1)
          ifNot: Mem(32)
            storeValue: Mem(47), Val(6)
