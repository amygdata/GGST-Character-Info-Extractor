beginSubroutine: s32'SIN_HaseiBegin'
  if: Val(3)
    modifyVar2: (DIV), Val(15000), Val(3)
    modifyVar: (SUB), Mem(25), Mem(0)
    endIf: 
  else: 
    modifyVar: (SUB), Mem(25), Val(15000)
    endElse: 
  modifyVar: (ADD), Mem(28), Mem(330)
  ifOperation: (IS_GREATER), Mem(28), Val(180)
    storeValue: Mem(28), Val(180)
    endIf: 
  endSubroutine: 

