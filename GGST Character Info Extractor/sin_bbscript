beginSubroutine: s32'StillGrowing'
  setRandom: 2, 46, 0, 1, 0, 15
  ifOperation: (IS_LESSER_OR_EQUAL), Mem(46), Val(15)
    storeValue: Mem(47), Val(1)
    endIf: 
  ifOperation: (IS_LESSER_OR_EQUAL), Mem(46), Val(10)
    storeValue: Mem(47), Val(2)
    endIf: 
  ifOperation: (IS_LESSER_OR_EQUAL), Mem(46), Val(6)
    storeValue: Mem(47), Val(3)
    endIf: 
  ifOperation: (IS_LESSER_OR_EQUAL), Mem(46), Val(3)
    storeValue: Mem(47), Val(4)
    endIf: 
  ifOperation: (IS_EQUAL), Mem(46), Val(1)
    storeValue: Mem(47), Val(5)
    endIf: 
  if: Mem(206)
    storeValue: Mem(47), Mem(206)
    endIf: 
  setRandom: 2, 46, 0, 1, 0, 15
  ifOperation: (IS_LESSER_OR_EQUAL), Mem(46), Val(15)
    storeValue: Mem(48), Val(1)
    endIf: 
  ifOperation: (IS_LESSER_OR_EQUAL), Mem(46), Val(10)
    storeValue: Mem(48), Val(2)
    endIf: 
  ifOperation: (IS_LESSER_OR_EQUAL), Mem(46), Val(6)
    storeValue: Mem(48), Val(3)
    endIf: 
  ifOperation: (IS_LESSER_OR_EQUAL), Mem(46), Val(3)
    storeValue: Mem(48), Val(4)
    endIf: 
  ifOperation: (IS_EQUAL), Mem(46), Val(1)
    storeValue: Mem(48), Val(5)
    endIf: 
  if: Mem(207)
    storeValue: Mem(48), Mem(207)
    endIf: 
  ifOperation: (IS_EQUAL), Mem(45), Val(1)
    modifyVar: (MUL), Mem(47), Val(2)
    modifyVar: (MUL), Mem(48), Val(60)
    modifyVar: (ADD), Mem(48), Val(60)
    createArgHikitsukiVal: 2, 47, 2, 48
    createObjectWithArg: s32'StillGrowing_buffA', (PLAYER)
    endIf: 
  ifOperation: (IS_EQUAL), Mem(45), Val(2)
    storeValue: Mem(53), Mem(47)
    modifyVarAfterOperation: (MUL), Mem(48), Val(60), Mem(54)
    modifyVar: (ADD), Mem(54), Val(60)
    endIf: 
  ifOperation: (IS_EQUAL), Mem(45), Val(3)
    modifyVar: (MUL), Mem(47), Val(300)
    addTensionUnscaled: 2, 47
    endIf: 
  ifOperation: (IS_EQUAL), Mem(45), Val(4)
    modifyVar: (MUL), Mem(47), Val(2)
    modifyVar: (MUL), Mem(48), Val(60)
    modifyVar: (ADD), Mem(48), Val(60)
    createArgHikitsukiVal: 2, 47, 2, 48
    createObjectWithArg: s32'StillGrowing_buffB', (PLAYER)
    endIf: 
  ifOperation: (IS_EQUAL), Mem(45), Val(5)
    modifyVar: (MUL), Mem(47), Val(5)
    modifyVar: (ADD), Mem(77), Mem(47)
    endIf: 
  endSubroutine: 

