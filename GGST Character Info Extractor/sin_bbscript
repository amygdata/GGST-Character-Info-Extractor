beginSubroutine: s32'CmnCutSceneDamage_Kakusei'
  ifOpponentCharacterMain: s16'pot'
  if: Mem(0)
    debugLogForScript: s32'pot', 0, 0
    endIf: 
  ifOpponentCharacterMain: s16'fau'
  if: Mem(0)
    debugLogForScript: s32'fau', 0, 0
    endIf: 
  endSubroutine: 

beginSubroutine: s32'OnFrameStep'
  callSubroutine: s32'cmnOnFrameStep'
  if: Val(3)
    ifOperation: (IS_LESSER), Mem(25), Val(5000)
      storeValue: Mem(27), Val(100)
      endIf: 
    ifOperation: (IS_GREATER_OR_EQUAL), Mem(25), Val(5000)
      storeValue: Mem(27), Val(70)
      endIf: 
    ifOperation: (IS_GREATER_OR_EQUAL), Mem(25), Val(10000)
      storeValue: Mem(27), Val(40)
      endIf: 
    modifyVar2: (DIV), Val(15000), Val(3)
    ifOperation: (IS_LESSER), Mem(25), Mem(0)
      storeValue: Mem(26), Val(1)
      endIf: 
    else: 
      if: Mem(26)
        storeValue: Mem(26), Val(0)
        endIf: 
      endElse: 
    ifNot: Mem(139)
      ifNot: Mem(329)
        ifNot: Mem(81)
          if: Mem(28)
            modifyVar: (SUB), Mem(28), Val(1)
            endIf: 
          else: 
            ifOperation: (IS_LESSER), Mem(25), Val(15000)
              if: Val(300)
                modifyVar2: (DIV), Val(15000), Val(300)
                modifyVar2: (MUL), Mem(0), Mem(27)
                modifyVar2: (DIV), Mem(0), Val(100)
                modifyVar: (ADD), Mem(25), Mem(0)
                endIf: 
              else: 
                modifyVar: (ADD), Mem(25), Val(15000)
                endElse: 
              endIf: 
            ifOperation: (IS_LESSER), Mem(25), Val(0)
              storeValue: Mem(25), Val(0)
              endIf: 
            endElse: 
          endIf: 
        endIf: 
      endIf: 
    endIf: 
  else: 
    storeValue: Mem(26), Val(0)
    endElse: 
  ifOperation: (IS_EQUAL), Mem(208), Val(-1)
    storeValue: Mem(26), Val(1)
    endIf: 
  if: Mem(54)
    ifNot: Mem(139)
      ifNot: Mem(329)
        ifNot: Mem(81)
          modifyVar: (ADD), Mem(54), Val(-1)
          ifOperation: (IS_EQUAL), Mem(53), Val(1)
            walkFSpeed: 6800
            walkBSpeed: 5150
            initDashFSpeed: 14500
            fDashAccelSpeed: 580
            endIf: 
          ifOperation: (IS_EQUAL), Mem(53), Val(2)
            walkFSpeed: 7300
            walkBSpeed: 5400
            initDashFSpeed: 15000
            fDashAccelSpeed: 600
            endIf: 
          ifOperation: (IS_EQUAL), Mem(53), Val(3)
            walkFSpeed: 7800
            walkBSpeed: 5650
            initDashFSpeed: 15500
            fDashAccelSpeed: 620
            endIf: 
          ifOperation: (IS_EQUAL), Mem(53), Val(4)
            walkFSpeed: 8300
            walkBSpeed: 5900
            initDashFSpeed: 16000
            fDashAccelSpeed: 640
            endIf: 
          ifOperation: (IS_EQUAL), Mem(53), Val(5)
            walkFSpeed: 8800
            walkBSpeed: 6150
            initDashFSpeed: 16500
            fDashAccelSpeed: 660
            endIf: 
          endIf: 
        endIf: 
      endIf: 
    endIf: 
  else: 
    storeValue: Mem(53), Val(0)
    walkFSpeed: 6300
    walkBSpeed: 4900
    initDashFSpeed: 14000
    fDashAccelSpeed: 560
    endElse: 
  getTrainingItemVal: s32'TRP_SIN', 2, 157
  if: Mem(157)
    ifOperation: (IS_EQUAL), Mem(157), Val(1)
      if: Mem(89)
        storeValue: Mem(25), Val(15000)
        endIf: 
      endIf: 
    ifOperation: (IS_EQUAL), Mem(157), Val(2)
      storeValue: Mem(25), Val(15000)
      endIf: 
    endIf: 
  endSubroutine: 

