beginState: s32'CmnActMatchWin'
  upon: (IMMEDIATE)
    callSubroutine: s32'cmnactmatchwin'
    playAnimation: s32'sin800cs', 0
    checkLastStateName: s32'WinActionWait'
    if: Mem(0)
      playCameraAnimation: s32'sin800cs', 23, 0, -1
      endIf: 
    else: 
      playCameraAnimation: s32'sin800cs', 23, 15, -1
      endElse: 
    cinesco: 1
    swapMeshSet: s32'800cs'
    endUpon: 
  sprite: s32'null', 2147483647
  upon: (FRAME_STEP)
    ifOperation: (IS_EQUAL), Mem(105), Val(0)
      zLine: 2, 5
      endIf: 
    ifOperation: (IS_EQUAL), Mem(105), Val(1)
      resetSaturation: 1, 1
      setPostProcessSaturation: -1, 800, 50
      resetDiffusion: 1, 1
      setDiffusionLuminance: -1, 1400, 50
      setDiffusionThreshold: -1, 0, 50
      characterSfx: s32'SE_BTL_SIN_MatchWin_00'
      endIf: 
    ifOperation: (IS_EQUAL), Mem(105), Val(20)
      resetDOF: 1, 1
      focalDistance: -1, 700, 0
      focalRegion: -1, 0, 0
      focalNearTransition: -1, 0, 0
      focalFarTransition: -1, 500, 0
